{
  "appenders": {
    "out": {
      "type": "stdout",
      "layout": {
        "type": "pattern",
        "pattern": "\[\e[32m\]%d{yyyy-MM-dd hh:mm:ss} %p %f{1} %l\[\e[0m\] %m"
      }
    }
  },
  "categories": {
    "default": {
      "appenders": [
        "out"
      ],
      "level": "info",
      "enableCallStack": true
    }
  }
}
